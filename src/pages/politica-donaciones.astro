---
import PublicLayout from '@/layouts/PublicLayout.astro';
import DonationPolicy from '@/landing/DonationPolicy';
const robots = ["index", "follow"];
const title = "Política de Donaciones | NaviFest 2025";
const description = "Evento benéfico realizado por el centro Pastoral de la Universidad Nacional del Santa";
const keywords = ["Navifest", "benéfico"];
const url = "https://navifest.vercel.app/politica-donaciones";
const imageOG = "/gift.svg";
const imagetwt = "/gift.svg";
---

<PublicLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
    <DonationPolicy client:load />
</PublicLayout>
